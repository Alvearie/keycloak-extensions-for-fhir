{
	"keycloak": {
		"serverUrl": "http://keycloak:8080/auth",
		"adminUser": "<KEYCLOAK_USER>",
		"adminPassword": "<KEYCLOAK_PASSWORD>",
		"adminClientId": "admin-cli",
